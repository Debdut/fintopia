hnologies Inc. in August 2016. Dell Technologies Inc. was founded in 1984 and is headquartered in Round Rock, Texas.","isStatic":true,"ipoDate":"2016-08-01","name":"Dell Technologies Inc.","website":"https://www.delltechnologies.com","sector":"Technology","symbol":"DELL"},"cashFlowStatement":[{"period":"FY","year":"2022","data":{"deferredIncomeTax":-365000000,"commonStockIssued":334000000,"netChangeInCash":0,"debtRepayment":-26723000000,"stockBasedCompensation":1622000000,"netCashUsedForInvestingActivites":1306000000,"dividendsPaid":-2240000000,"otherWorkingCapital":-11186000000,"commonStockRepurchased":-663000000,"investmentsInPropertyPlantAndEquipment":0,"otherFinancingActivites":12683000000,"purchasesOfInvestments":-414000000,"otherInvestingActivites":1207000000,"acquisitionsNet":0,"netIncome":5707000000,"accountsPayables":5742000000,"changeInWorkingCapital":-621000000,"salesMaturitiesOfInvestments":513000000,"otherNonCashItems":-10894000000,"netCashProvidedByOperatingActivities":0,"accountsReceivables":-2193000000,"depreciationAndAmortization":4551000000,"inventory":-2514000000,"netCashUsedProvidedByFinancingActivities":-16609000000,"effectOfForexChangesOnCash":-106000000}},{"year":"2021","period":"FY","data":{"netIncome":3250000000,"investmentsInPropertyPlantAndEquipment":0,"otherNonCashItems":-3466000000,"netCashUsedProvidedByFinancingActivities":-5950000000,"accountsPayables":1598000000,"debtRepayment":-20919000000,"netCashProvidedByOperatingActivities":11407000000,"depreciationAndAmortization":5390000000,"stockBasedCompensation":1609000000,"changeInWorkingCapital":5023000000,"commonStockIssued":452000000,"acquisitionsNet":1763000000,"dividendsPaid":0,"otherInvestingActivites":-2054000000,"accountsReceivables":-396000000,"deferredIncomeTax":-399000000,"otherWorkingCapital":-10565000000,"purchasesOfInvestments":-338000000,"inventory":-243000000,"commonStockRepurchased":-1604000000,"otherFinancingActivites":16121000000,"netCashUsedForInvestingActivites":-460000