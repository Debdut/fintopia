 products under the Autronica, Det-Tronics, Edwards, Fireye, GST, Kidde, LenelS2, Marioff, Onity, and Supra; Carrier, Automated Logic, Bryant, CIAT, Day \u0026 Night, Heil, NORESCO, and Riello; and Carrier Commercial Refrigeration, Carrier Transicold, and Sensitech brands. The company was incorporated in 2019 and is headquartered in Palm Beach Gardens, Florida.","symbol":"CARR","website":"https://www.corporate.carrier.com","sector":"Industrials","exchangeShort":"NYSE","ipoDate":"2020-03-17","isStatic":true,"name":"Carrier Global Corporation"},"incomeStatement":[{"data":{"incomeBeforeTax":2400000000,"netIncome":1664000000,"incomeTaxExpense":699000000,"eps":1.92,"grossProfitRatio":0.29010818415562994,"totalOtherIncomeExpensesNet":-245000000,"grossProfit":5980000000,"epsdiluted":1.87,"weightedAverageShsOutDil":890300000,"operatingIncomeRatio":0.1283170814534517,"sellingAndMarketingExpenses":0,"otherExpenses":0,"weightedAverageShsOut":867700000,"costOfRevenue":14633000000,"operatingExpenses":3623000000,"generalAndAdministrativeExpenses":0,"researchAndDevelopmentExpenses":503000000,"operatingIncome":2645000000,"netIncomeRatio":0.0807257555911318,"revenue":20613000000,"sellingGeneralAndAdministrativeExpenses":3120000000},"year":"2021","period":"FY"},{"period":"FY","data":{"eps":2.29,"operatingExpenses":3239000000,"generalAndAdministrativeExpenses":0,"costOfRevenue":12347000000,"netIncome":1982000000,"revenue":17456000000,"operatingIncome":3083000000,"sellingAndMarketingExpenses":0,"weightedAverageShsOutDil":880200000,"incomeTaxExpense":849000000,"sellingGeneralAndAdministrativeExpenses":0,"grossProfit":5109000000,"weightedAverageShsOut":866500000,"epsdiluted":2.25,"incomeBeforeTax":2855000000,"netIncomeRatio":0.11354262144821264,"grossProfitRatio":0.2926787351054079,"totalOtherIncomeExpensesNet":-228000000,"researchAndDevelopmentExpenses":419000000,"operatingIncomeRatio":0.17661549037580201,"otherExpenses":2820000000},"year":"2020"},{"year":"2019","data":{"eps":2.05,"gene